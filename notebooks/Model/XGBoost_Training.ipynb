    "total_combinations = np.prod([len(v) for v in param_dist.values()])\n",
    "n_iter = 50\n",
    "\n",
    "\n",
    "\n",
    "# Sample parameters\n",
    "param_list = list(ParameterSampler(param_dist, n_iter=n_iter, random_state=42))\n",
    "\n",
    "# Progress tracking\n",
